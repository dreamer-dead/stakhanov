{
  "process_rules": {
    "stick_to_parent": [
      {
        "command_line_patterns": [
          "^.*python\\.exe$",
          "^--multiprocessing-fork$"
        ]
      },
      {
        "command_line_patterns": [
          "^.*python(\\.exe|\\.bat)?$",
          "^.*rcssmin.py$"
        ]
      }
    ]
  },
  "files_storage": {
    "storage_dir": "C:\\test_st_cache",
    "max_file_size_bytes": 10485760,
    "safe_to_link_extensions": [".obj", ".pch", ".exe", ".dll"]
  },
  "output_filtering": {
    "safe_to_drop_output_extensions": [".ilk"]
  }
}
