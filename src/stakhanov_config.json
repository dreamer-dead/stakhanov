{
  "process_rules": {
    "stick_to_parent": [
      {
        "command_line_patterns": [
          "^.*python\\.exe$",
          "^--multiprocessing-fork$"
        ]
      },
      {
        "command_line_patterns": [
          "^.*python(\\.exe|\\.bat)?$",
          "^.*rcssmin.py$"
        ]
      }
    ],
    "do_not_track": [
      {
        "command_line_patterns": [ "^.*vctip\\.exe$" ]
      },
      {
        "command_line_patterns": [ "^.*mspdbsrv\\.exe$" ]
      }
    ]
  },
  "files_storage": {
    "storage_dir": "C:\\test_st_cache",
    "max_file_size_bytes": 50000000,
    "safe_to_link_extensions": [".obj", ".pch", ".exe", ".dll", ".ninja"]
  },
  "files_filtering": {
    "safe_to_drop_output_extensions": [".ilk", ".pyc"],
    "safe_to_drop_input_extensions": [".pyc"]
  }
}
